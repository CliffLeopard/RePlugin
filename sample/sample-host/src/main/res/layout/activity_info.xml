<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InfoActivity"
    tools:ignore="MissingDefaultResource">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_plugin_info"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="插件ActivityInfo"
        android:onClick="clickPluginInfo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_host_info"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="宿主ActivtiyInfo"
        android:onClick="clickHostInfo"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_plugin_info"
         />

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cleo.sample.plugin">

    <application
        android:name=".PluginApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Replugin">
        <activity
            android:name=".activity.HookActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CommonTestActivity"
            android:exported="false" />
        <activity
            android:name=".activity.DealActivity"
            android:exported="true" />
        <activity
            android:name=".activity.BroadcastActivity"
            android:exported="true" />
        <activity
            android:name=".activity.ProviderActivity"
            android:exported="true" />
        <activity
            android:name=".activity.IdentifyActivity"
            android:exported="true" />
        <activity
            android:name=".activity.CFragmentActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>